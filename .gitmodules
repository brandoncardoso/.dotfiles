[submodule ".vim/bundle/Vundle.vim"]
	path = vim/.vim/bundle/Vundle.vim
	url = https://github.com/VundleVim/Vundle.vim.git
[submodule "neovim/.config/nvim/vim-plug"]
	path = neovim/.config/nvim/vim-plug
	url = https://github.com/junegunn/vim-plug
[submodule "polybar/.config/polybar/scripts/polybar-pulseaudio-control"]
	path = polybar/.config/polybar/scripts/polybar-pulseaudio-control
